{
  "name": "node-db-migration",
  "version": "1.2.4",
  "description": "Database migration script for nodejs for sqlite3, mysql, pg",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md",
    "LICENSE.txt",
    "package.json"
  ],
  "scripts": {
    "test": "mocha",
    "report": "nyc mocha",
    "lcov": "nyc report --reporter=text-lcov > coverage.lcov",
    "codecov": "codecov",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "rm ./lib -rf && tsc -p src/tsconfig.json",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akoidan/node-db-migration.git"
  },
  "typings": "./lib/index.d.ts",
  "keywords": [
    "db",
    "migration"
  ],
  "author": "Andrew Koidan <deathangel908@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akoidan/node-db-migration/issues"
  },
  "homepage": "https://github.com/akoidan/node-db-migration#readme",
  "dependencies": {
    "moment": "*"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.3",
    "@types/mysql": "^2.15.5",
    "@types/node": "^11.11.3",
    "@types/pg": "^7.4.13",
    "@types/sqlite3": "^3.1.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mysql": "*",
    "nyc": "^13.3.0",
    "pg": "*",
    "sqlite3": "*",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "gts": "^0.9.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
